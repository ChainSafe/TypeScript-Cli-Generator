import {ICliCommand} from "../../<%= "../".repeat(2 * depth) %>util";
import {IGlobalArgs} from "../../<%= "../".repeat(2 * depth) %>options";
import { I<%= name %>Args, <%= name.toLowerCase() %>Options } from "./options";
import {<%= name.toLowerCase() %>Handler} from "./handler";<%- -%>
<% for (let i = 0; i < subCommands.length; i++) { %>
import {<%= subCommands[i].name.toLowerCase() %>} from "./cmds/<%= subCommands[i].name.toLowerCase() %>";<%- -%>
<% } %>

export const <%= name.toLowerCase()%>: ICliCommand<Record<never, never>, I<%= name %>Args & IGlobalArgs > = {
  command: "<%= name.toLowerCase() %>",
  
  describe: "<%= description %>",

  examples: [
    {
      // TODO: Change this!
      command: "",
      description: "",
    },
  ],

  options: <%= name.toLowerCase() %>Options,
  <% if (subCommands && subCommands.length > 0) { %>
  subcommands: [<%= subCommands.map(x => x.name.toLowerCase()) %>]
  <% } else { %>
  handler: <%= name.toLowerCase() %>Handler<%- -%>
  <% } %>
};